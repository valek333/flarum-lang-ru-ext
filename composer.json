{
	"name": "valek333/flarum-lang-ru-ext",
	"type": "flarum-extension",
	"description": "Rus extension language pack for Flarum.",
	"homepage": "https://github.com/valek333/flarum-lang-ru-ext/",
	"keywords": [
		"locale",
		"flarum"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Valentin Kuharenko",
			"email": "v.m.kuharenko@yandex.ru",
			"homepage": "https://github.com/valek333/"
		}
	],
	"support": {
		"issues": "https://github.com/valek333/flarum-lang-ru-ext/issues"
	},
	"require": {
		"flarum/core": "^1.3"
	},
	"minimum-stability": "stable",
	"extra": {
		"flarum-extension": {
			"title": "RusExt",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "ru",
			"title": "Русский"
		}
	}
}
