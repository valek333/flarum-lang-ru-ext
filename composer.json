	"description": "Rus extension language pack for Flarum.",
	"homepage": "https://github.com/valek333/flarum-lang-ru-my/",
	"keywords": [
		"locale",
		"flarum",
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Valentin Kuharenko",
			"email": "v.m.kuharenko@yandex.ru",
			"homepage": "https://github.com/valek333/"
		}
	],
	"support": {
		"issues": "https://github.com/valek333/flarum-lang-ru-my/issues",
	},
	"require": {
		"flarum/core": "^1.3"
	},
	"extra": {
		"flarum-extension": {
			"title": "RusExt",
